        "@sencha/ext-modern-treegrid": "^7.0.0",
        "@sencha/ext-react-modern": "^7.1.0",
        "@sencha/ext-react-renderercell": "^7.0.0",
        "@sencha/ext-react-transition": "^7.0.0",
        "clipboard": "^2.0.4",
        "prettier-standalone": "^1.3.1-0",
        "prop-types": "^15.7.2",
        "react": "~16.8.6",
        "react-dom": "~16.8.6",
        "react-router-dom": "^5.0.0"
    },
