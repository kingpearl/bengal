{
    "name": "@sencha/extjs-to-react-converter",
    "version": "7.0.0",
    "author": "Sencha, Inc.",
    "description": "Converts Ext JS code snippets to JSX/React",
    "license": "MIT",
    "main": "index.js",
    "private": true,
    "scripts": {
        "clean": "rimraf build",
        "start": "npm run dev",
        "dev": "webpack-dev-server --env.verbose=no --env.environment=development",
        "prod": "webpack-dev-server --env.verbose=no --env.environment=production",
        "build": "npm run clean && cross-env webpack --env.environment=production"
    },
    "extbuild": {
        "defaultprofile": "",
